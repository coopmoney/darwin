{
  "theme": "dark",
  "showGettingStarted": false,
  "raycastPreferredWindowMode": "default",
  "raycastShouldFollowSystemAppearance": false,
  "raycastWindowPresentationMode": 0,
  "popToRootTimeout": 60,
  "emojiSkinTone": "neutral",
  "resultViewStyle": "large",
  "showActionPanel": true,
  "showActionShortcuts": true,
  "showExtensionDescriptions": true,
  "navigationCommandStyleIdentifierKey": "default",
  "inputPlaceholder": "Search for apps, commands, and more...",

  "hotkey": {
    "key": "space",
    "modifiers": ["cmd"]
  },

  "raycastGlobalHotkey": {
    "key": "space",
    "modifiers": ["cmd"]
  },

  "searchBarPosition": "center",
  "showMenuBarIcon": true,

  "extensions": {
    "clipboard-history": {
      "enabled": true,
      "hotkey": {
        "key": "v",
        "modifiers": ["cmd", "shift"]
      }
    },
    "snippets": {
      "enabled": true
    },
    "calculator": {
      "enabled": true
    },
    "system": {
      "enabled": true
    },
    "calendar": {
      "enabled": true
    },
    "file-search": {
      "enabled": true,
      "hotkey": {
        "key": "f",
        "modifiers": ["cmd", "shift"]
      }
    },
    "window-management": {
      "enabled": true
    }
  },

  "suggestionOrdering": {
    "default": "recency"
  },

  "showRecentApplications": true,
  "showFrequentApplications": true,

  "advancedSettings": {
    "developerMode": false,
    "showDeveloperTools": false
  },

  "accessibility": {
    "reduceMotion": false
  },

  "analytics": {
    "enabled": true
  }
}
